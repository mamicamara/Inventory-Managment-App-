<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="379.0" prefWidth="483.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <Label fx:id="lblTitle" layoutX="48.0" layoutY="45.0" text="Add Part" textFill="#3a5373">
      <font>
         <Font name="System Bold" size="12.0" />
      </font>
   </Label>
   <RadioButton fx:id="rbtnInHouse" layoutX="194.0" layoutY="45.0" mnemonicParsing="false" onAction="#handleInHouseRadio" text="In-House">
      <toggleGroup>
         <ToggleGroup fx:id="toggleGrpSource" />
      </toggleGroup>
   </RadioButton>
   <RadioButton fx:id="rbtnOutsourced" layoutX="316.0" layoutY="45.0" mnemonicParsing="false" onAction="#handleOutsourcedRadio" text="Outsourced" toggleGroup="$toggleGrpSource" />
   <Label layoutX="92.0" layoutY="86.0" text="ID" />
   <Label layoutX="94.0" layoutY="121.0" text="Name" />
   <Label layoutX="96.0" layoutY="155.0" text="Inv" />
   <Label layoutX="96.0" layoutY="188.0" text="Price/Cost" />
   <Label layoutX="95.0" layoutY="224.0" text="Max" />
   <Label fx:id="lblCompanyMachine" layoutX="94.0" layoutY="263.0" text="Machine ID" />
   <Label layoutX="284.0" layoutY="223.0" text="Min" />
   <TextField fx:id="tbxId" disable="true" layoutX="194.0" layoutY="81.0" prefHeight="26.0" prefWidth="200.0" style="-fx-border-insets: 5;" />
   <TextField fx:id="tbxName" layoutX="194.0" layoutY="116.0" prefHeight="26.0" prefWidth="200.0" style="-fx-border-insets: 5;" />
   <TextField fx:id="tbxInv" layoutX="194.0" layoutY="150.0" prefHeight="26.0" prefWidth="200.0" style="-fx-border-insets: 5;" />
   <TextField fx:id="tbxPrice" layoutX="194.0" layoutY="183.0" prefHeight="26.0" prefWidth="200.0" style="-fx-border-insets: 5;" />
   <TextField fx:id="tbxMax" layoutX="194.0" layoutY="219.0" prefHeight="26.0" prefWidth="70.0" style="-fx-border-insets: 5;" />
   <TextField fx:id="tbxMin" layoutX="323.0" layoutY="219.0" prefHeight="26.0" prefWidth="70.0" style="-fx-border-insets: 5;" />
   <TextField fx:id="tbxCompanyOrMachine" layoutX="194.0" layoutY="258.0" prefHeight="26.0" prefWidth="200.0" style="-fx-border-insets: 5;" />
   <Button fx:id="btnSave" layoutX="249.0" layoutY="315.0" mnemonicParsing="false" onAction="#handleSaveButton" prefHeight="26.0" prefWidth="70.0" text="Save" />
   <Button fx:id="btnCancel" layoutX="330.0" layoutY="315.0" mnemonicParsing="false" onAction="#handleCancelButton" prefHeight="26.0" prefWidth="70.0" text="Cancel" />
</AnchorPane>
